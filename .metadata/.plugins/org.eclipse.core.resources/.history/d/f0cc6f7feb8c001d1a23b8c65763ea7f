package com.firstproject.org;

public class MyClass {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyClass ob = new MyClass();
		int res = ob.sum(2, 4)
		System.out.println(res);
		System.out.println("res");
		System.out.println("res");
	}

	public int sum(int a, int b) {
		return (a+b);
	}


}
